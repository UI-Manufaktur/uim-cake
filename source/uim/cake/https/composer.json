{
    "name": "UIM/http",
    "description": "UIM HTTP client and PSR7/15 middleware libraries",
    "type": "library",
    "keywords": [
        "UIM",
        "http",
        "psr7",
        "psr15"
    ],
    "homepage": "https://UIM.org",
    "license": "MIT",
    "authors": [
        {
            "name": "UIM Community",
            "homepage": "https://github.com/UIM/http/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/UIM/UIM/issues",
        "forum": "https://stackoverflow.com/tags/UIM",
        "irc": "irc://irc.freenode.org/UIM",
        "source": "https://github.com/UIM/http"
    },
    "require": {
        "php": ">=7.2.0",
        "UIM/core": "^4.0",
        "UIM/event": "^4.0",
        "UIM/utility": "^4.0",
        "composer/ca-bundle": "^1.2",
        "psr/http-client": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "laminas/laminas-diactoros": "^2.1",
        "laminas/laminas-httphandlerrunner": "^1.0"
    },
    "provide": {
        "psr/http-client-implementation": "^1.0"
    },
    "suggest": {
        "UIM/cache": "To use cache session storage",
        "UIM/orm": "To use database session storage"
    },
    "autoload": {
        "psr-4": {
            "Cake\\Http\\": "."
        }
    }
}
