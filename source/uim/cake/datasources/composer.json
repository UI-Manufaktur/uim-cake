{
    "name": "UIM/datasource",
    "description": "Provides connection managing and traits for Entities and Queries that can be reused for different datastores",
    "type": "library",
    "keywords": [
        "UIM",
        "datasource",
        "connection management",
        "entity",
        "query"
    ],
    "homepage": "https://UIM.org",
    "license": "MIT",
    "authors": [
        {
            "name": "UIM Community",
            "homepage": "https://github.com/UIM/datasource/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/UIM/UIM/issues",
        "forum": "https://stackoverflow.com/tags/UIM",
        "irc": "irc://irc.freenode.org/UIM",
        "source": "https://github.com/UIM/datasource"
    },
    "require": {
        "php": ">=7.2.0",
        "UIM/core": "^4.0",
        "psr/log": "^1.1",
        "psr/simple-cache": "^1.0"
    },
    "suggest": {
        "UIM/utility": "If you decide to use EntityTrait.",
        "UIM/collection": "If you decide to use ResultSetInterface.",
        "UIM/cache": "If you decide to use Query caching."
    },
    "autoload": {
        "psr-4": {
            "Cake\\Datasource\\": "."
        }
    }
}
